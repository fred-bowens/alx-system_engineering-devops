Host myserver
  HostName my.server.ip.or.hostname
  User ubuntu
  IdentityFile ~/.ssh/school
  PasswordAuthentication no


ssh -i ~/.ssh/school ubuntu@your.server.ip.or.hostname
cd ~/.ssh
nano authorized_keys
ssh-rsa AAAAB3NzaC1yc2EAAAADAQABAAACAQCy
chmod 700 ~/.ssh
chmod 600 ~/.ssh/authorized_keys
